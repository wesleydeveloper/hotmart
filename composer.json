{
    "name": "wesleydeveloper/hotmart",
    "description": "Hotmart Package Laravel Unofficial",
    "license": "MIT",
    "authors": [
        {
            "name": "Wesley Silva",
            "email": "wesley_silva@outlook.com",
            "homepage": "http://wesleysilva.me"
        }
    ],
    "homepage": "https://github.com/wesleydeveloper/hotmart",
    "keywords": ["laravel", "hotmart"],
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/wesleydeveloper/hotmart"
        }
    ],
    "require": {
        "illuminate/support": "~5",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Wesleydeveloper\\Hotmart\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wesleydeveloper\\Hotmart\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wesleydeveloper\\Hotmart\\HotmartServiceProvider"
            ],
            "aliases": {
                "Hotmart": "Wesleydeveloper\\Hotmart\\Facades\\Hotmart"
            }
        }
    },
    "minimum-stability": "dev"
}
